{
  "name": "react-redux-undo",
  "version": "0.0.1",
  "description": "Redux undo library",
  "keywords": [
    "react",
    "undo",
    "redux",
    "middleware",
    "redux-middleware"
  ],
  "repository": "github:wagnercsfilho/react-redux-undo",
  "bugs": "https://github.com/wagnercsfilho/react-redux-undo/issues",
  "homepage": "https://github.com/wagnercsfilho/react-redux-undo",
  "author": "Wagner CS Filho <wagnercsfilho@gmail.com>",
  "main": "dist/react-redux-undo.cjs.js",
  "module": "dist/react-redux-undo.esm.js",
  "browser": "dist/react-redux-undo.umd.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "rollup": "^1.29.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "3.8.3"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false
}
